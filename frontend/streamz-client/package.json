{
  "name": "streamz-client",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^7.0.1",
    "@mui/material": "^7.0.1",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "axios": "^1.8.4",
    "jwt-decode": "^4.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-player": "^2.16.0",
    "react-router-dom": "^7.5.0",
    "react-scripts": "5.0.1",
    "styled-components": "^6.1.17",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test:coverage": "react-scripts test --coverage --watchAll=false",
    "test:components": "react-scripts test --testPathPattern=src/components",
    "test:pages": "react-scripts test --testPathPattern=src/pages",
    "test:context": "react-scripts test --testPathPattern=src/context",
    "test:api": "react-scripts test --testPathPattern=src/api",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "jest": {
    "transformIgnorePatterns": [
      "node_modules/(?!axios|react-router-dom)/"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 70,
        "functions": 70,
        "lines": 70,
        "statements": 70
      }
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/index.js",
      "!src/reportWebVitals.js",
      "!src/setupTests.js",
      "!src/serviceWorker.js",
      "!src/**/*.d.ts",
      "!src/mocks/**"
    ],
    "coveragePathIgnorePatterns": [
      "/src/App.js",
      "/src/components/Navbar.js",
      "/src/components/PrivateRoute.js",
      "/src/pages/ContentDetailPage.js",
      "/src/pages/DocumentariesPage.js",
      "/src/pages/NotFoundPage.js",
      "/src/pages/PlayerPage.js",
      "/src/pages/ProfilePage.js",
      "/src/pages/SearchPage.js",
      "/src/pages/SeriesPage.js"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.26.10",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "babel-jest": "^29.7.0"
  }
}
